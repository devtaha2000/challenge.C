#include <stdlib.h>
#include <math.h>

float didtance (int a, int b , int c ,int d){

          return(sqrt(pow((c-a),2)+pow((d-b)));
}


 main()
{

    int xa , ya , xb , yb ;
    printf("entrer les cordonnees : \n");
     printf("entrer xa");
