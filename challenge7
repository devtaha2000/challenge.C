#include <stdio.h>
#include <stdlib.h>

void main()
{
    int a = 5 , b=10,r,s,t,f,m ;
    r=a+b  ;
    s=a-b ;
    t=a*b;
    f=a/b;
    m=a%b;
     int opiration;
    printf("entrer votre opiration:");
    scanf("%d %d %d %d %d ",&r,&s,&t,&f,&m);
      if (opiration==r){
            printf("%d",a+b);
       }

      if (opiration==s){
            printf("%d",a-b);
      }
       if (opiration==t){
            printf("%d",a*b);
       }
         if (opiration==f){
            printf("%d",a/b);
       }
        if (opiration==m){
            printf("%d",a%b);
       }





}
