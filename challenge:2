   #include <stdio.h>
#include<stdlib.h>
 
int main()
{
     
       float celsius ,fahrenheit;
    

     
       printf("celsius=");
       scanf("%f",&celsius);
         
       fahrenheit =(celsius*1.8) + 32;
       printf("%f",fahrenheit);


    return 0;
}

            












    return 0;
}
